tab-pane fade show active\" id=\"buy\" role=\"tabpanel\">\n    <div class=\"section mt-2\">\n      <div class=\"card\">\n        <div class=\"card-body\">\n          <form @submit.prevent.enter=\"\">\n            <h2>How much STRLY do you want to buy?</h2>\n            <div class=\"form-group basic\">\n              <label class=\"label\">Enter Amount</label>\n              <div class=\"input-group mb-1\">\n                <div class=\"input-group-prepend\">\n                  <span style=\"font-size: 44px !important;\" class=\"input-group-text\" id=\"input1\">$</span>\n                </div>\n                <input @keyup=\"convert\" v-model=\"amount\" style=\"font-size: 40px !important;\" type=\"text\" class=\"text-center form-control form-control-lg\">\n              </div>\n            </div>\n            <div class=\"form-group basic\">\n              <h3 id=\"toStrly\" class=\"text-center p-1\" style=\"color:#444;background:#e3e3e3\">0.00 STRLY</h3>\n              <button :disabled=\"disabled\" class=\"btn btn-outline-primary btn-block btn-lg\" type=\"button\">BUY</button>\n            </div>\n          </form>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport api from '../api'\nexport default {\n  name: 'Buy',\n  data() {\n    return { amount:0, value: 0.0000, disabled:true }\n  },\n  methods:{\n    convert(event) {\n      this.isDisabled()\n      api.convertUsdToStrata(event.target.value || 0)\n      .then(response=>{\n        document.querySelector(\"#toStrly\").innerHTML = `${parseFloat(response).toFixed(6)} STRLY`\n      })\n    },\n    buyStrata() { },\n    isDisabled() {\n      if(this.amount != 0) this.disabled = false\n      else this.disabled = true\n    }\n  },\n  created() {\n    api.getRates().then(response => this.value = response.current_value)\n  }\n}\n</script>\n\n<style scoped>\n\n</style>"],"sourceRoot":"src/components"}]}