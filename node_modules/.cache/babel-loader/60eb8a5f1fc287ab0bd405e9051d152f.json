r(var i=0;i<str.length;i++) {\n        if(i != 0 && (i % 3) == 0) res.push(',')\n        res.push(str[i])\n      }\n      return `${res.join('')}.`+decimals\n    }\n  },\n  methods: {\n    onSelectCoin(coin) { this.select = coin }\n  },\n  created() {\n    api.fetchAllCoins().then(coins => {\n      // console.log(coins);\n      this.coins = coins\n    })\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n\n</style>"],"sourceRoot":"src/views"}]}