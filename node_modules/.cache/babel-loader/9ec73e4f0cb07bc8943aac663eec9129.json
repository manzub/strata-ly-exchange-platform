CAAA,QAAA,EAAA,cAAA,IAAA,CAAA,OAAA,EAAA;AACA,KAVA;AAWA,IAAA,eAXA,6BAWA;AAAA,aAAA,KAAA,WAAA,CAAA,IAAA,IAAA,KAAA,IAAA,CAAA,IAAA,CAAA,aAAA,GAAA,mBAAA,GAAA,iBAAA;AAAA;AAXA,IAJA;AAiBA,EAAA,OAjBA,qBAiBA;AACA,QAAA,SAAA,GAAA,KAAA,MAAA,CAAA,MAAA,CAAA,EAAA;AACA,QAAA,IAAA,GAAA,KAAA,IAAA,CAAA,OAAA,CAAA,IAAA;AACA,SAAA,WAAA,GAAA,IAAA,CAAA,IAAA,CAAA,UAAA,IAAA;AAAA,aAAA,IAAA,CAAA,IAAA,IAAA,SAAA;AAAA,KAAA,CAAA;AACA;AArBA,CAAA","sourcesContent":["<template>\n  <div>\n    <Header />\n    <div id=\"appCapsule\">\n      <div class=\"section mt-2 mb-2\">\n        <div class=\"listed-detail mt-3\">\n          <div class=\"icon-wrapper\">\n            <div :class=\"iconbox\"> <ion-icon :name='icon' /> </div>\n          </div>\n          <h3 class=\"text-center mt-2\">{{ transactionStat }}</h3>\n        </div>\n        <ul class=\"listview flush transparent simple-listview no-space mt-3\">\n          <li>\n            <strong>Status</strong>\n            <span :class=\"status\">{{ transaction.status }}</span>\n          </li>\n          <li>\n            <strong>From</strong>\n            <span>{{ transaction.from == auth.info.walletAddress ? \"Me\" : transaction.from.substr(0,20) }}...</span>\n          </li>\n          <li>\n            <strong>To</strong>\n            <span>{{ transaction.to == auth.info.walletAddress ? \"Me\" : transaction.to.substr(0,20) }}...</span>\n          </li>\n          <li>\n            <strong>Date</strong>\n            <span>{{ date }}</span>\n          </li>\n          <li>\n            <strong>Amount</strong>\n            <h3 class=\"m-0\">{{ transaction.value }} STRLY</h3>\n          </li>\n        </ul>\n      </div>\n    </div>\n    <app-bottom-menu />\n  </div>\n</template>\n\n<script>\nimport { mapGetters } from 'vuex';\nimport AppBottomMenu from '../components/AppBottomMenu.vue';\nimport Header from '../components/Header.vue'\n\nexport default {\n  name: 'transactionDetail',\n  data() { return { transaction: null } },\n  components: { Header, AppBottomMenu },\n  computed: {\n    ...mapGetters(['auth']),\n    status() { return this.transaction.status == 'complete' ? 'text-success' : 'text-info' },\n    icon() {\n      return this.transaction.from == this.auth.info.walletAddress ? 'arrow-forward-outline' : 'arrow-down-outline'\n    },\n    iconbox() { return this.transaction.from == this.auth.info.walletAddress ? 'iconbox bg-primary' : 'iconbox bg-success' },\n    date() {\n      const date = new Date(this.transaction.timestamp)\n      return `${date.getFullYear()}-${date.getMonth()}-${date.getDate()}`\n    },\n    transactionStat() { return this.transaction.from == this.auth.info.walletAddress ? 'Transfered Strata' : 'Recieved Strata' }\n  },\n  created() {\n    const trnx_hash = this.$route.params.id;\n    const logs = this.auth.records.logs\n    this.transaction = logs.find(trnx => trnx.hash == trnx_hash)\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n\n</style>"],"sourceRoot":"src/views"}]}