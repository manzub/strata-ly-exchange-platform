\n         rate() {\n            return api.convertRate(this.coin)\n         }\n      },\n      methods: {\n         select() {\n            this.$emit('selectCoin', this.coin.name)\n         }\n      },\n      created() {\n         this.strly_value = JSON.parse(window.localStorage.getItem('rates')).current_value\n         api.convertUsdToStrata(1).then(response => { this.to1usd = parseInt(response) })\n      }\n   }\n</script>\n\n<style scoped>\n\n</style>"]}]}