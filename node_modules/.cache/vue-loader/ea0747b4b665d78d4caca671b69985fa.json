on-box bg-success' },\n    iconName() { return this.transaction.from == this.auth.info.walletAddress ? 'arrow-forward-outline' : 'arrow-down-outline'},\n    title() { return this.transaction.from == this.auth.info.walletAddress ? 'Transfered' : 'Recieved'},\n    value_stats() { return this.transaction.from == this.auth.info.walletAddress ? 'price text-danger' : 'price text-success'},\n    sub_msg() {\n      var sub = this.transaction.from.substr(55,this.transaction.from.length)\n      return this.transaction.from == this.auth.info.walletAddress ? `To...${sub}` : `From...${sub}`\n    },\n    value() {\n      var amount = `${this.transaction.value}`.substr(0,5)\n      return this.transaction.from == this.auth.info.walletAddress ? `-${amount}..` : `+${amount}..`\n    }\n  },\n}\n</script>"]}]}