ns = response;\n        let coins = []\n        response.forEach(coin => {\n          let num = coin.value*0.0002;\n          var rate = Number(coin.value)+num;\n          var this_coin = {...coin,rate}\n          coins.push(this_coin);\n        })\n        this.coins = coins;\n      })\n    }\n  }\n</script>"]}]}